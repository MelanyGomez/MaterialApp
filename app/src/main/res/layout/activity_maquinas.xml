<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".Maquinas">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <!--button5-->
        <Button
            android:id="@+id/materialButton2"
            android:layout_width="120dp"
            android:layout_height="60dp"
            android:backgroundTint="@color/black1"
            android:onClick="irMaterialMaq"
            android:text="MATERIAL"
            android:textColor="@color/white"
            android:textSize="15dp"
            app:cornerRadius="0dp"
            app:layout_constraintEnd_toStartOf="@+id/maquinariaButton2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <!--button7-->
        <Button
            android:id="@+id/maquinariaButton2"
            android:layout_width="120dp"
            android:layout_height="60dp"
            android:backgroundTint="@color/black1"
            android:ellipsize="end"
            android:gravity="center"
            android:maxLines="1"
            android:text="MAQUINAS"
            android:textColor="@color/white"
            android:textSize="15dp"
            app:cornerRadius="0dp"
            app:layout_constraintEnd_toStartOf="@+id/registroButton2"
            app:layout_constraintTop_toTopOf="parent" />
        <!--button8-->
        <Button
            android:id="@+id/registroButton2"
            android:layout_width="120dp"
            android:layout_height="60dp"
            android:backgroundTint="@color/black1"
            android:onClick="irRegistroMaq"
            android:text="REGISTRO"
            android:textColor="@color/white"
            android:textSize="15dp"
            app:cornerRadius="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    </LinearLayout>

    <TextView
        android:id="@+id/p2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="128dp"
        android:text="maquinas"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.537"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

</androidx.constraintlayout.widget.ConstraintLayout>